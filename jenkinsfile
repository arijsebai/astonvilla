pipeline {
    agent any 
    stages {
        stage ("clean workspace"){
            steps {
                sh 'rm -rf /home/arij/.jenkins/workspace/astonvilla/*'
                sh 'ls'
               
            }
        }
        stage ("clone project"){
            steps {
                sh 'git clone https://github.com/arijsebai/astonvilla.git'
                sh 'ls'
                
            }
        }
       } 
}

