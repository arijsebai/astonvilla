pipeline {
    agent any 
    stages {
        stage ("clean workspace"){
            steps {
                sh 'rm -rf /home/arij/.jenkins/workspace/astonvilla/*'
                sh 'ls'
                sh 'pwd'
            }
        }
        stage ("clone project"){
            steps {
                sh 'git clone '
                sh 'ls'
                sh 'pwd'
            }
        }
       } 
}

